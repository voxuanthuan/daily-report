{
  "name": "jira-daily-report",
  "displayName": "jira-daily-report",
  "description": "Generate Jira daily standup reports - TUI CLI tool",
  "version": "0.1.138",
  "publisher": "thuanvo",
  "author": {
    "name": "Thuan Vo",
    "email": "voxuanthuan@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "jira",
    "daily-report",
    "standup",
    "tempo",
    "productivity",
    "cli",
    "tui"
  ],
  "homepage": "https://github.com/voxuanthuan/daily-report#readme",
  "bugs": {
    "url": "https://github.com/voxuanthuan/daily-report/issues"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/voxuanthuan/daily-report.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "jira-report": "./dist/cli/index.js"
  },
  "files": [
    "dist/",
    "images/",
    "CLI.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "check-types": "tsc --noEmit",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.19",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "commander": "^12.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "neo-blessed": "^0.2.0",
    "open": "^10.0.3"
  },
  "optionalDependencies": {
    "clipboardy": "^4.0.0"
  }
}
